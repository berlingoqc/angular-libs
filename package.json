{
  "name": "@berlingoqc/ng-lib",
  "description": "Project for all of @berlingoqc librairies",
  "version": "2.2.29",
  "scripts": {
    "build:all": "ng build common && ng build loopback && ng build autoform && ng build autotable && ng build notification && ng build pwa",
    "build:all:prod": "ng build --configuration production common && ng build --configuration production loopback && ng build --configuration production autoform && ng build --configuration production autotable",
    "build:demo": "ng build --configuration production demo",
    "publishversion": "cd projects/$NAME/ && npm version $TYPE && cd ../../ && ng build --configuration production $NAME && cd dist/$NAME && npm publish",
    "postpublishversion": "./scripts/publish.sh",
    "documentation": "compodoc -p ./projects/autoform/tsconfig.lib.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "formatting:check": "prettier --check .",
    "formatting:write": "prettier --write .",
    "version": "node -p \"require('./package.json').version\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/berlingoqc/angular-libs/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@angular/service-worker": "^15.0.4",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "ngx-permissions": "^13.0.1",
    "object-hash": "^3.0.0",
    "prismjs": "^1.28.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular-devkit/core": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@angular/language-service": "^15.0.4",
    "@compodoc/compodoc": "^1.1.14",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^12.12.38",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^15.0.3",
    "prettier": "^2.1.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}